<?xml version="1.0" encoding="ISO-8859-1" ?>
<adfc-mobile-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.0">
  <task-flow-definition id="contacts">
    <default-activity>search</default-activity>
    <view id="search">
      <page>/com.blogspot.lucbors.book.ch11.contact/search.amx</page>
    </view>
    <view id="details">
      <page>/com.blogspot.lucbors.book.ch11.contact/details.amx</page>
    </view>
    <control-flow-rule id="__5">
      <from-activity-id>search</from-activity-id>
      <control-flow-case id="__6">
        <from-outcome>details</from-outcome>
        <to-activity-id>details</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
  </task-flow-definition>
</adfc-mobile-config>

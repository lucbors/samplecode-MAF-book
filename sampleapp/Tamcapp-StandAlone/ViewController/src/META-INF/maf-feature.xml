            <adfmf:amx file="com.tamcapp.mobilebook.Organization/OrganizationTaskFlow.xml#OrganizationTaskFlow"/>
        </adfmf:content>
    </adfmf:feature>
    <adfmf:feature id="com.tamcapp.mobilebook.Maps" name="Maps" image="images/Map-and-Location.png"
                   icon="images/Map-and-Location.png">
        <adfmf:content id="com.tamcapp.mobilebook.Maps.1">
            <adfmf:amx file="com.tamcapp.mobilebook.Maps/MapsTaskFlow.xml#MapsTaskFlow"/>
        </adfmf:content>
        <adfmf:deviceFeatureAccess>
            <adfmf:deviceLocation access="true" id="dl1"/>
        </adfmf:deviceFeatureAccess>
    </adfmf:feature>
    <adfmf:feature id="com.tamcapp.mobilebook.SocialMedia" name="SocialMedia" image="images/Twitter-Bird.png"
                   icon="images/Twitter-Bird.png">
        <adfmf:content id="com.tamcapp.mobilebook.SocialMedia.1">
            <adfmf:localHTML url="com.tamcapp.mobilebook.SocialMedia/twitter.html"/>
        </adfmf:content>
    </adfmf:feature>

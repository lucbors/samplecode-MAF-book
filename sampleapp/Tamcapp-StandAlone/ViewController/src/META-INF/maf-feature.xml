            <adfmf:amx file="com.tamcapp.mobilebook.Schedule/ScheduleTaskFlow.xml#ScheduleTaskFlow"/>
        </adfmf:content>
    </adfmf:feature>
    <adfmf:feature id="com.tamcapp.mobilebook.Organization" name="Organization" image="images/organization.png"
                   icon="images/organization.png">

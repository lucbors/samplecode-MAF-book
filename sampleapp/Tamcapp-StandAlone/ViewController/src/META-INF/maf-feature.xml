                </adfmf:includes>
            </adfmf:amx>
        </adfmf:content>
    </adfmf:feature>
    <adfmf:feature id="com.tamcapp.mobilebook.Schedule" name="Schedule" image="images/Calendar.png"
                   icon="images/Calendar.png">
